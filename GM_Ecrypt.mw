a := 2:
while not(evalb(LegendreSymbol(a,p) = -1) and evalb(LegendreSymbol(a,q) = -1)) do
	a +=1:
	end: 
N := p*q:

Encrypt := proc (publicKey, m)
	local r := rand(1..publicKey[1])():
	local c:
	if evalb(m=0) then
		c:= r^2 mod publicKey[1]: #if bit=0, r^2 mod N, a square residue
	elif evalb(m=1) then
		c:= publicKey[2]*r^2 mod publicKey[1]: #if bit !=0, ar^2 mod N, a non-square residue
		end:#if
	return c
	end:
Decrypt := proc (privateKey, c)
	local m:
	if evalb(LegendreSymbol(c,privateKey[1]) =1) then
		m := 0: #if square residue
	else
		m:=1:
		end:#if not
	return m
	end:
c := Encrypt([N,a],0);
Decrypt([p,q],c);#expect to get 0

c := Encrypt([N,a],1);
Decrypt([p,q],c);#expect to get 1
</EC-CodeEditor>
