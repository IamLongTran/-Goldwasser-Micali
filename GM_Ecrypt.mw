<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" size="16" layout="Heading 1"><Font size="16">(Square Roots mod p - the Hard Case)</Font></Text-field></Title><Text-field style="Text" layout="Normal"></Text-field><Text-field style="Text" layout="Normal">When <Equation executable="false" style="2D Math" input-equation="" display=""></Equation>there are more complicated ways of computing square roots mod <Equation executable="false" style="2D Math" input-equation="" display=""></Equation></Text-field><Text-field style="Text" layout="Normal"><Font encoding="UTF-8">Implementation of Tonelli\342\200\223Shanks algorithm (see http://en.wikipedia.org/wiki/Tonelli%E2%80%93Shanks_algorithm). </Font></Text-field><Text-field style="Text" layout="Normal"></Text-field>
<Group labelreference="L481" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#restart;</Text-field>
</Input>
</Group>
<Group labelreference="L490" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#with(numtheory);</Text-field>
</Input>
</Group><CodeEditor-ExecGroup view="presentation" hide-input="false" labelreference="L509" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">restart;
#with(numtheory):
with(NumberTheory):
with(StringTools):

fact2 := proc(p) #factor 2, for some reasons I cannot extract int from ifactor, so I def my own func
	local s := convert(convert(p, binary),string):
	local l:= length(s):
	local e := [SearchAll(&quot;1&quot;,s)]:
	local S := (e[-1]):
	local Q := convert([seq(2^(e[-1]-i), i = e[1...-1])], `+`):
	return S,Q
end:


myRESSOL := proc(a, p)
	if evalb(p=0) then
		return 0
	elif evalb(p=2) then
		return a
	elif not(evalb(isprime(p))) then
		return false
		end: #if evalb(p=0)
	
	local S, Q:
	if evalb(LegendreSymbol(a,p) = 1) then
		S,Q := fact2(p-1):
		if evalb(S=1) then #cover problem 1 implementation
			local e := (p+1)/4:
			local res := a^e mod p:
			return (res, -res): 
			end: #if evalb(S=1)
	local z := 1:
	while evalb(LegendreSymbol(z,p) &lt;&gt; -1) do
		z += 1:
		end: #end while
	local M := S:
	local c := z^Q mod p:
	local t := a^Q mod p:
	local R := a^((Q+1)/2) mod p:

	while evalb(t &lt;&gt; 1) do
		local i,e := (0,2):
		for i from 1 to (M-1) do
			if evalb(t^e mod p = 1) then 
				return [R, p-R]
				end: #end if
			e := e*2:
			end: #end for
		local b:= c^(2^(M-i-1)) mod p:
		M := i:
		c := b^2 mod p:
		t := t*b^2 mod p:
		R := R*b mod p:	
		end: #end while
	return [R, p-R]
		
	else
		return false
		end: #end evalb(LegendreSymbol(a,p) = 1)
	end: #end proc
#test cases
myRESSOL(5,41);
msolve(x^2 = 5,41);

myRESSOL(6,41); #no soln
msolve(x^2 = 6,41); #no soln

myRESSOL(5,1009); 
msolve(x^2 = 5, 1009);

myRESSOL(269,1009); 
msolve(x^2 = 269, 1009);

myRESSOL(5,1031); 
msolve(x^2 = 5, 1031);

myRESSOL(269,1031); 
msolve(x^2 = 269, 1031);

(* Explain
 In my opinion, my implmentation of Tonelli-Shanks is a &quot;well-rounded&quot; version, meaning it can compute square root residue of any prime p
 (p=2 =&gt; return a because any number is a square root residue of p
 the logic is similar to problem 1 explaination, however
 this implementation can solve for p != 3 mod 4 because it has additional loop searching for a quadratic residue
 Once LegendreSymbol oor Jacobi ditactes (z/p) = 1
 return result and its congruences p-res
 *)</EC-CodeEditor>
